<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_margin="10dp"
    android:background="@drawable/zt_adapter_card_back" >
    <RelativeLayout
        android:id="@+id/rl_top"
        android:layout_width="match_parent"
        android:layout_height="@dimen/kaipian_top_ll_height"
        android:background="@drawable/card_shape_top"
        android:gravity="center_vertical"

        android:orientation="horizontal" >

        <TextView
            android:id="@+id/tv_card_head_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="大北京"
            android:layout_centerVertical="true"
            android:paddingLeft="@dimen/title_right_toright"
            android:textColor="#ffffff"
            android:textSize="@dimen/fab_actions_spacing" />

        <ImageView
            android:id="@+id/tv_card_head_meter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:src="@drawable/welding_line"
             />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/rl_top"
        android:orientation="vertical"
        >


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"

                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="编号："
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="12dp"

                        android:textColor="#ffffff"
                        android:textSize="@dimen/fab_plus_icon_size" />

                    <TextView
                        android:id="@+id/tv_jiance_data"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="大北京大北京大北京大北京大北京大北京大北京"
                        android:singleLine="true"
                        android:layout_marginBottom="12dp"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/fab_plus_icon_size" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="线别："
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="12dp"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/fab_plus_icon_size" />

                    <TextView
                        android:id="@+id/tv_xianbie"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="大北京"
                        android:singleLine="false"
                        android:layout_marginBottom="12dp"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/fab_plus_icon_size" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="伤损位置："
                        android:layout_marginBottom="12dp"
                        android:layout_marginLeft="12dp"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/fab_plus_icon_size" />

                    <TextView
                        android:id="@+id/tv_shangsun_weizhi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="大北京"
                        android:singleLine="false"
                        android:layout_marginBottom="12dp"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/fab_plus_icon_size" />
                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="线名："
                        android:layout_marginBottom="12dp"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/fab_plus_icon_size" />

                    <TextView
                        android:id="@+id/tv_line_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="大北京"
                        android:singleLine="false"
                        android:layout_marginBottom="12dp"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/fab_plus_icon_size" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="行别："
                        android:layout_marginBottom="12dp"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/fab_plus_icon_size" />

                    <TextView
                        android:id="@+id/tv_hangbie"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="大北京"
                        android:singleLine="false"
                        android:layout_marginBottom="12dp"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/fab_plus_icon_size" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="伤损类型："
                        android:layout_marginBottom="12dp"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/fab_plus_icon_size" />

                    <TextView
                        android:id="@+id/tv_shangsun_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="大北京"
                        android:layout_marginBottom="12dp"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/fab_plus_icon_size" />
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:layout_marginLeft="12dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="伤损详情："
                android:textColor="#ffffff"
                android:textSize="@dimen/fab_plus_icon_size" />
            <TextView
                android:id="@+id/tv_shangsun_xiangq"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="大北京"
                android:textColor="#ffffff"
                android:textSize="@dimen/fab_plus_icon_size" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:layout_marginLeft="12dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="单位："
                android:textColor="#ffffff"
                android:textSize="@dimen/fab_plus_icon_size" />
            <TextView
                android:id="@+id/tv_danwei"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="大北京"
                android:textColor="#ffffff"
                android:textSize="@dimen/fab_plus_icon_size" />
        </LinearLayout>
    </LinearLayout>


</RelativeLayout>