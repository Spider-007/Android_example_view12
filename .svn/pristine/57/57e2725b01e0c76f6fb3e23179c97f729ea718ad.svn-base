<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:segmentedgroup="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <!-- title -->
    <include
        android:id="@+id/system_title_tab"
        layout="@layout/system_title" />
	<!-- timebar -->
	<LinearLayout
        android:id="@+id/layout_supply_timebar"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="#E9F3FF"
        android:orientation="horizontal" >

        <ImageButton
            android:id="@+id/before_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/btn_prev_pre" />

        <TextView
            android:id="@+id/supply_time_textview"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="60"
            android:gravity="center"
            android:textColor="@color/black"
            android:textSize="19sp" />

        <ImageButton
            android:id="@+id/behind_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/btn_next_pre" />
    </LinearLayout>

  	<!--   用抽屉叠加的控件 -->
    <FrameLayout
        android:id="@+id/framelayout_supplt"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:id="@+id/slidingDrawer_content_ll"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#000832"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/handle_ll_time"
                android:layout_width="fill_parent"
                android:layout_height="70dp"
                android:layout_margin="8dp"
                android:background="@color/transparent"
                android:gravity="center"
                android:orientation="horizontal" >

                <com.radio.SegmentedGroup
                    android:id="@+id/radio_group"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    segmentedgroup:sc_border_width="1dp"
                    segmentedgroup:sc_corner_radius="5dp"
                    segmentedgroup:sc_tint_color="#0081fc" >

                    <RadioButton
                        android:id="@+id/rb_yue"
                        style="@style/RadioButton"
                        android:layout_width="0dp"
                        android:layout_height="33dp"
                        android:layout_weight="1"
                        android:checked="true"
                        android:text="月度" />

                    <RadioButton
                        android:id="@+id/rb_year"
                        style="@style/RadioButton"
                        android:layout_width="0dp"
                        android:layout_height="33dp"
                        android:layout_weight="1"
                        android:text="年度" />
                </com.radio.SegmentedGroup>
            </LinearLayout>

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#2D334A" />

            <LinearLayout
                android:id="@+id/handle_ll_text"
                android:layout_width="fill_parent"
                android:layout_height="110dp"
                android:layout_marginTop="40dp"
                android:background="@color/transparent"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:padding="8dp" >

                <LinearLayout
                    android:id="@+id/project_ll"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="2dp"
                        android:text="项目											 "
                        android:textColor="@color/white"
                        android:textSize="16sp" />

                    <AutoCompleteTextView
                        android:id="@+id/project_autocompleteTextView"
                        android:layout_width="220dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="2dp"
                        android:completionThreshold="1"
                        android:hint="请输入项目名称"
                        android:singleLine="true"
                        android:textColor="@color/white"
                        android:background="@color/transparent"
                        android:textColorHint="@color/white"
                        android:textSize="16sp" />
                </LinearLayout>

                <TextView
                    android:layout_marginTop="0dp"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="@color/white" />

                <LinearLayout
                    android:id="@+id/build_ll"
                    android:layout_marginTop="30dp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="2dp"
                        android:text="建设单位    "
                        android:textColor="@color/white"
                        android:textSize="16sp" />

                    <AutoCompleteTextView
                        android:id="@+id/build_autocompleteTextView"
                        android:layout_width="220dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="2dp"
                        android:completionThreshold="1"
                        android:hint="请输入建设单位名称"
                        android:background="@color/transparent"
                        android:singleLine="true"
                        android:textColor="@color/white"
                        android:textColorHint="@color/white"
                        android:textSize="16sp" />
                </LinearLayout>

                <TextView
                    android:layout_marginTop="0dp"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="@color/white" />
            </LinearLayout>
            
             <LinearLayout
                android:id="@+id/handle_ll_date"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="40dp"
                android:background="@color/transparent"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:padding="8dp" >
            
            <com.htmitech.emportal.ui.plugin.widget.WheelView
                android:id="@+id/year"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:visibility="gone"
                 />

            <com.htmitech.emportal.ui.plugin.widget.WheelView
                android:id="@+id/month"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:visibility="gone" />
            
            </LinearLayout>
        </LinearLayout>

        <SlidingDrawer
            android:id="@+id/slidingDrawer_supplt"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:content="@+id/content"
            android:handle="@+id/layout_supplt" >

            <include
                android:id="@id/content"
                layout="@layout/supply_content" />

            <LinearLayout
                android:id="@+id/layout_supplt"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/drawer_back"
                android:gravity="center"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="33dp"
                    android:background="@drawable/drop_down_bg"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingBottom="1dp"
                    android:paddingLeft="100dp"
                    android:paddingRight="100dp"
                    android:paddingTop="1dp" >

                    <ImageView
                        android:id="@+id/imageviewHandle_supplt"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:paddingTop="8dp"
                        android:src="@drawable/btn_drop" />
                </LinearLayout>
            </LinearLayout>
        </SlidingDrawer>
    </FrameLayout>

</LinearLayout> <!-- sliding_bg  sliding_drawer -->
