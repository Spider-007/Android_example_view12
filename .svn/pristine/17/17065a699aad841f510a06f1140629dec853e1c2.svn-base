<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_margin="10dp"
    android:background="@color/transparent" >
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/dark_blue"/>
    <RelativeLayout
        android:id="@+id/rl_top"
        android:layout_width="match_parent"
        android:layout_height="@dimen/kaipian_top_ll_height"
        android:background="@drawable/trans"
        android:gravity="center_vertical"
        android:orientation="horizontal" >
        <ImageView
            android:id="@+id/iv_logo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_cz" />
        <TextView
            android:id="@+id/tv_history_child_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="伤损处置"
            android:layout_toRightOf="@id/iv_logo"
            android:layout_centerVertical="true"
            android:paddingLeft="@dimen/title_right_toright"
            android:textColor="@color/white"
            android:textSize="@dimen/fab_actions_spacing" />

        <TextView
            android:id="@+id/tv_history_child_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="10-19"
            android:layout_toLeftOf="@+id/iv_button"
            android:layout_marginRight="@dimen/fontsize_heading12"
            android:textColor="@color/white"
            android:layout_centerVertical="true"
            android:textSize="@dimen/fab_actions_spacing"
             />
            <ImageView
                android:id="@+id/iv_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/fontsize_heading12"
                android:layout_alignParentRight="true"
                android:src="@drawable/btn_down"/>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/ll_center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/rl_top"
        android:animateLayoutChanges="true"
        android:orientation="vertical"
        >
        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/dark_blue"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/header_footer_top_bottom_padding"
            android:layout_marginTop="@dimen/header_footer_top_bottom_padding"
            android:layout_marginLeft="@dimen/header_footer_left_right_padding"

            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="处置措施："
                android:textColor="#ffffff"
                android:textSize="@dimen/fab_plus_icon_size" />
            <TextView
                android:id="@+id/tv_czcs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="大北京"
                android:textColor="#ffffff"
                android:textSize="@dimen/fab_plus_icon_size" />
        </LinearLayout>

        <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/header_footer_top_bottom_padding"
        android:layout_marginLeft="@dimen/header_footer_left_right_padding"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="处置车间："
            android:textColor="#ffffff"
            android:textSize="@dimen/fab_plus_icon_size" />
        <TextView
            android:id="@+id/tv_czcj"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="大北京"
            android:textColor="#ffffff"
            android:textSize="@dimen/fab_plus_icon_size" />
    </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/header_footer_top_bottom_padding"
            android:layout_marginLeft="@dimen/header_footer_left_right_padding"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="施工负责人："
                android:textColor="#ffffff"
                android:textSize="@dimen/fab_plus_icon_size" />
            <TextView
                android:id="@+id/tv_sgfzr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="大北京"
                android:textColor="#ffffff"
                android:textSize="@dimen/fab_plus_icon_size" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/header_footer_top_bottom_padding"
            android:layout_marginLeft="@dimen/header_footer_left_right_padding"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="备注："
                android:textColor="#ffffff"
                android:textSize="@dimen/fab_plus_icon_size" />
            <TextView
                android:id="@+id/tv_bz"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="大北京"
                android:textColor="#ffffff"
                android:textSize="@dimen/fab_plus_icon_size" />
        </LinearLayout>

    </LinearLayout>

</RelativeLayout>