<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:from="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:orientation="vertical"
    tools:context="zhp.plugin.first.MainActivity">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:text="泰康人寿总指标"
        android:textColor="@color/white" />


    <mobilereport.com.chatkit.myView.FromLayout
        android:id="@+id/fromlayout"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="#fff"
        android:orientation="vertical"
        from:from_text_align="center"
        from:from_title="【2016/12/26 10:48 单位：万元】"
        from:from_title_back="@color/colorAccent"
        from:from_title_text_align="center"
        from:from_title_text_color="@color/huise"
        from:from_title_text_size="14sp"
        from:from_watermark_back_color="#C5C1AA"
        from:from_watermark_displayType="repeat"
        from:from_watermark_show="true"
        from:from_watermark_text="水印背景仝玉池" />
</LinearLayout>
