<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/allbackground"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/tv_planned_rate_adapter_gcmc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="20dp"
        android:text="武钢"
        android:textSize="15sp" />

    <TextView
        android:layout_width="1.5dp"
        android:layout_height="match_parent"
        android:layout_marginLeft="20dp"
        android:background="@color/white" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="20dp">

        <com.htmitech.ztcustom.zt.view.TextProgressBar
            android:id="@+id/progress_planned_rate_configweight"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="wrap_content"
            android:layout_height="12dp"
            android:max="100"
            android:progressDrawable="@color/planned_rate_bar_qjht" />

        <TextView
            android:id="@+id/tv_planned_rate_configweight"
            android:layout_width="wrap_content"
            android:layout_height="12dp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="1dp"
            android:layout_toRightOf="@id/progress_planned_rate_configweight"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:text=""
            android:textColor="@color/white"
            android:textSize="10sp" />

        <com.htmitech.ztcustom.zt.view.TextProgressBar
            android:id="@+id/progress_planned_rate_contractweight"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="wrap_content"
            android:layout_height="12dp"
            android:layout_below="@id/progress_planned_rate_configweight"
            android:max="100"
            android:progressDrawable="@color/planned_rate_bar_ljfhs" />

        <TextView
            android:id="@+id/tv_planned_rate_contractweight"
            android:layout_width="wrap_content"
            android:layout_height="12dp"
            android:layout_below="@id/progress_planned_rate_configweight"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="1dp"
            android:layout_toRightOf="@id/progress_planned_rate_contractweight"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:text=""
            android:textColor="@color/white"
            android:textSize="10sp" />

        <com.htmitech.ztcustom.zt.view.TextProgressBar
            android:id="@+id/progress_planned_rate_deliveryweight"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="wrap_content"
            android:layout_height="12dp"
            android:layout_below="@id/progress_planned_rate_contractweight"
            android:max="100"
            android:progressDrawable="@color/planned_rate_bar_ljdhs" />

        <TextView
            android:id="@+id/tv_planned_rate_deliveryweight"
            android:layout_width="wrap_content"
            android:layout_height="12dp"
            android:layout_below="@id/tv_planned_rate_contractweight"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="1dp"
            android:layout_toRightOf="@id/progress_planned_rate_deliveryweight"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:text=""
            android:textColor="@color/white"
            android:textSize="10sp" />

        <com.htmitech.ztcustom.zt.view.TextProgressBar
            android:id="@+id/progress_planned_rate_qjcontractweight"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="wrap_content"
            android:layout_height="12dp"
            android:layout_below="@id/progress_planned_rate_deliveryweight"
            android:max="100"
            android:progressDrawable="@color/planned_rate_bar_sjfhs" />

        <TextView
            android:id="@+id/tv_planned_rate_qjcontractweight"
            android:layout_width="wrap_content"
            android:layout_height="12dp"
            android:layout_below="@id/tv_planned_rate_deliveryweight"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="1dp"
            android:layout_toRightOf="@id/progress_planned_rate_qjcontractweight"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:text=""
            android:textColor="@color/white"
            android:textSize="10sp" />

    </RelativeLayout>


</LinearLayout>