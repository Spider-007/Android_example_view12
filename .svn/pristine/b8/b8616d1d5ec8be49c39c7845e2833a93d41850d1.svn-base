<?xml version="1.0" encoding="utf-8"?>
<com.htmitech.emportal.ui.detail.DetailActivityLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:from="http://schemas.android.com/apk/res-auto"
    xmlns:skin="http://schemas.android.com/tools"
    android:id="@+id/dal_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/color_ffffffff"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <include
            android:id="@+id/system_title"
            layout="@layout/system_title" />
        <com.htmitech.MyView.EmptyLayout
            android:id="@+id/layout_down"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="@color/white"
            android:orientation="vertical">
            <TextView
                android:id="@+id/tv_center_soso_title"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/layout_daiban_titlebar"
                android:textColor="#555555"
                android:gravity="center"
                android:textSize="18sp"
                android:visibility="gone"
                android:background="#ECEEF5"/>
            <mobilereport.com.chatkit.myView.FromLayout
                android:id="@+id/fromlayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/tv_center_soso_title"
                android:orientation="vertical"
                from:from_text_align="center"
                from:from_title="【2016/12/26 10:48 单位：万元】"
                from:from_title_back="@color/colorAccent"
                from:from_title_text_align="center"
                from:from_title_text_color="@color/huise"
                from:from_title_text_size="16sp"
                from:from_watermark_back_color="#C5C1AA"
                from:from_watermark_displayType="repeat"
                from:from_watermark_show="true"
                from:from_watermark_text="水印背景仝玉池">

            </mobilereport.com.chatkit.myView.FromLayout>

            <LinearLayout
                android:id="@+id/ll_content_soso"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/layout_daiban_titlebar"
                android:background="@color/transparent"
                android:orientation="vertical"
                android:visibility="gone">


                <LinearLayout
                    android:id="@+id/content_soso"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical"></LinearLayout>

                <TextView
                    android:id="@+id/tv_buttom"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:minHeight="100dp" />
            </LinearLayout>

            <RelativeLayout
                android:id="@+id/layout_daiban_titlebar"
                android:layout_width="match_parent"
                android:layout_height="48dip"
                skin:enable="true"
                android:visibility="gone"
                android:background="@color/ht_hred_title">

                <ImageView
                    android:id="@+id/btn_back"
                    style="@style/title_button"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/header_bar_btn_selector"
                    android:onClick="onClickBack"
                    android:scaleType="center"
                    skin:enable="true"
                    android:src="@drawable/mx_btn_back" />

                <TextView
                    android:id="@+id/report_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="报表中心"
                    android:maxLength="12"
                    skin:enable="true"
                    android:textColor="@color/ht_title_text_color"
                    android:textSize="@dimen/max_textsize" />

                <TextView
                    android:id="@+id/tv_shaixuan"
                    style="@style/title_button"
                    android:layout_alignParentRight="true"
                    android:onClick="onClickShaixuan"
                    android:text="筛选"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    skin:enable="true"
                    android:visibility="gone" />

            </RelativeLayout>

            <com.htmitech.emportal.ui.widget.flatingactionButton.AddFloatingActionButton
                android:id="@+id/function"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_margin="20dp"
                android:layout_marginTop="100dp"
                android:scaleType="center"
                android:src="@drawable/btn_operation_homelabel_off"
                android:visibility="visible" />

            <TextView
                android:id="@+id/iv_no_add"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="数据不存在!"
                android:gravity="center_horizontal"
                android:visibility="gone"
                android:drawableTop="@drawable/img_no_add"
                android:layout_centerInParent="true"/>
        </com.htmitech.MyView.EmptyLayout>

        <LinearLayout
            android:id="@+id/ll_bottom_style"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_alignParentBottom="true"
            android:orientation="horizontal">

            <com.htmitech.app.widget.DrawableCenterTextView
                android:id="@+id/tv_share"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:background="@drawable/send_message"
                android:drawableLeft="@drawable/btn_action_share"
                android:drawablePadding="8dp"
                android:gravity="center_vertical"
                android:text="分享"
                android:textColor="@color/ht_hred_title"
                android:textSize="14sp" />

            <com.htmitech.app.widget.DrawableCenterTextView
                android:id="@+id/tv_select"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:background="@drawable/special_focus"
                android:drawableLeft="@drawable/btn_action_screening"
                android:drawablePadding="8dp"
                android:gravity="center_vertical"
                android:text="筛选"
                android:textColor="@color/ht_hred_title"
                android:textSize="14sp" />
        </LinearLayout>
    </LinearLayout>
</com.htmitech.emportal.ui.detail.DetailActivityLayout>