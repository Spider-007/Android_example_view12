<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:from="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/allbackground"
    tools:context="com.htmitech.ztcustom.zt.chinarailway.QualityBookActivity">


    <RelativeLayout
        android:id="@+id/ll_quality_book_title_theme"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_height"
        android:layout_alignParentTop="true"
        android:background="@color/title_color"
        android:gravity="center_vertical">

        <ImageButton
            android:id="@+id/ib_quality_book_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="#00000000"
            android:clickable="true"
            android:gravity="center"
            android:src="@drawable/btn_back" />

        <TextView
            android:id="@+id/tv_quality_book_title_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="质保书"
            android:textColor="#ffffff"
            android:textSize="@dimen/title_textsize" />

        <ImageView
            android:id="@+id/bt_quality_book_right_top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="8dp"
            android:src="@drawable/btn_search" />
    </RelativeLayout>


    <LinearLayout
        android:id="@+id/ll_quality_top"
        android:layout_width="match_parent"
        android:layout_height="@dimen/date_height"
        android:layout_marginTop="@dimen/title_height"
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">

        <ImageView
            android:id="@+id/iv_tanshangcompletesum_cut"
            android:layout_width="@dimen/datesize"
            android:layout_height="@dimen/datesize"
            android:src="@drawable/date_left" />

        <TextView
            android:id="@+id/tv_qualitybook_title_data"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="2012年5月"
            android:textColor="@color/black"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/iv_tanshangcompletesum_add"
            android:layout_width="@dimen/datesize"
            android:layout_height="@dimen/datesize"
            android:clickable="true"
            android:scaleType="fitCenter"
            android:src="@drawable/date_right" />
    </LinearLayout>


    <LinearLayout
        android:id="@+id/ll_card"
        android:layout_width="335dp"
        android:layout_height="132dp"
        android:layout_below="@+id/ll_quality_top"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="26dp"
        android:background="@drawable/card_shape"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:background="@drawable/card_shape_top"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_card_head_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:padding="5dp"
                android:text="全路质保书情况"
                android:textColor="#F8F8F8"
                android:textSize="@dimen/kaipian_top_textsize" />

            <TextView
                android:id="@+id/tv_card_head_meter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:padding="5dp"
                android:text="单位：公里"
                android:textColor="#ffffff"
                android:textSize="@dimen/kaipian_top_textsize"
                android:visibility="gone" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="5"
            android:gravity="center_vertical">

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <LinearLayout
                    android:id="@+id/tv_card_first_top_linear"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_card_first_top"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="当月数量 "
                        android:textColor="#888888"
                        android:textSize="@dimen/kaipian_top_textsize" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_gravity="center"
                        android:layout_marginLeft="1dp"
                        android:text="（份）"
                        android:textColor="#888888"
                        android:textSize="10sp" />

                </LinearLayout>


                <TextView
                    android:id="@+id/tv_card_first_bottom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_card_first_top_linear"
                    android:gravity="center"
                    android:textColor="#333333"
                    android:textSize="20sp" />

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:layout_alignTop="@+id/tv_card_first_top_linear"
                    android:layout_alignBottom="@id/tv_card_first_bottom"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@color/colorcardtopll" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <LinearLayout
                    android:id="@+id/tv_card_second_top_linear"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_card_second_top"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="当月总量"
                        android:textColor="#888888"
                        android:textSize="@dimen/kaipian_top_textsize" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text=" （万吨）"
                        android:layout_marginLeft="1dp"
                        android:textColor="#888888"
                        android:textSize="10dp" />


                </LinearLayout>

                <TextView
                    android:id="@+id/tv_card_second_bottom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_card_second_top_linear"
                    android:gravity="center"
                    android:textColor="#333333"
                    android:textSize="20sp" />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>


    <mobilereport.com.chatkit.myView.FromLayout
        android:id="@+id/fromlayoutqualitybook"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/ll_card"
        android:layout_marginTop="20dp"
        android:orientation="vertical"
        from:from_text_align="center"
        from:from_title="新发折断数量"
        from:from_title_back="#00000000"
        from:from_title_text_align="center"
        from:from_title_text_color="#FFF8F8F8"
        from:from_title_text_size="16sp"
        from:from_watermark_back_color="#C5C1AA"
        from:from_watermark_displayType="repeat"
        from:from_watermark_show="false"
        from:from_watermark_text="">

    </mobilereport.com.chatkit.myView.FromLayout>

</RelativeLayout>
