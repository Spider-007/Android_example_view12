<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:skin="http://schemas.android.com/android/skin"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.htmitech.emportal.ui.announcement.AnnouncementListActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/from_hight"
        skin:enable="true"
        android:background="@color/ht_hred_title"
        android:minHeight="50dp">

        <TextView
            android:id="@+id/tv_announcement_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:singleLine="true"
            android:textColor="@color/ht_title_text_color"
            skin:enable="true"
            android:textSize="17sp" />


        <ImageView
            android:id="@+id/iv_announcement_back"
            style="@style/title_button"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            skin:enable="true"
            android:scaleType="center"
            android:background="@drawable/mx_btn_back"
            />
        <ImageView
            android:id="@+id/iv_announcement_back_home"
            style="@style/title_button"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            skin:enable="true"
            android:scaleType="center"
            android:visibility="gone"
            android:background="@drawable/mx_btn_community_list_phone"
            />
    </RelativeLayout>
    <com.htmitech.MyView.EmptyLayout
        android:id="@+id/empty"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/color_daiban_yiban_back" >
        <com.htmitech.commonx.pulltorefresh.library.PullToRefreshListView
            android:id="@+id/announcement_listview"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </com.htmitech.commonx.pulltorefresh.library.PullToRefreshListView>
    </com.htmitech.MyView.EmptyLayout>
</LinearLayout>
