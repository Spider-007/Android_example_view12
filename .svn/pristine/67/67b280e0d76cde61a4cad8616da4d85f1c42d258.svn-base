<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_daiban_yiban_back" >
    <RelativeLayout
        android:id="@+id/daiban_rv_serach"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginBottom="6dp"
        android:layout_marginLeft="11dp"
        android:layout_marginRight="11dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/search_back"
        android:gravity="center" >

        <ImageView
            android:id="@+id/daiban_iv_serach"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleType="center"
            android:src="@drawable/mx_search_bar_icon_normal" />

        <TextView
            android:id="@+id/daiban_tv_serach"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/daiban_iv_serach"
            android:text="请输入标题关键字搜索"
            android:layout_centerVertical="true"
            android:textColor="#FFBEBEBE"
            android:textSize="14dp" />

    </RelativeLayout>

    <SearchView
        android:id="@+id/daiban_sv_search"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@null"
        android:iconifiedByDefault="false"
        android:imeOptions="actionSearch"
        android:inputType="textCapWords"
        android:queryHint="请输入要搜索的提标"
        android:searchMode="queryRewriteFromText"
        android:showAsAction="collapseActionView"
        android:visibility="gone" >
    </SearchView>


    <GridView
        android:id="@+id/daiban_gridview_daibanlist"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:padding="3dp"
        android:gravity="center"
        android:listSelector="@android:color/transparent"
        android:scrollbars="vertical"
        android:numColumns="4"
        android:paddingTop="@dimen/check_item_buttom5"
        android:layout_marginTop="38dp"
        android:stretchMode="columnWidth"
        android:background="@color/white"
        />


    <SlidingDrawer
        android:id="@+id/daiban_slidingDrawer_daiban_list"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="124dp"
        android:content="@+id/ll_general_from_child"
        android:handle="@+id/layout_daiban_list" >

        <!--<include-->
            <!--android:id="@id/content"-->
            <!--layout="@layout/slidingdrawer_content" />-->
        <LinearLayout
            android:id="@+id/ll_general_from_child"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:fitsSystemWindows="true"
            android:clipToPadding="true"
            android:orientation="vertical" >
        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout_daiban_list"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:background="@color/color_daiban_yiban_back"
            android:orientation="horizontal" >

            <LinearLayout
                android:id="@+id/ll_Handle_daiban_list"
                android:layout_width="fill_parent"
                android:layout_height="28dp"
                android:background="@drawable/btn_gest_bg"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingBottom="1dp"
                android:paddingLeft="100dp"
                android:paddingRight="100dp"
                android:paddingTop="1dp" >

                <ImageView
                    android:id="@+id/daiban_imageviewHandle_daiban_list"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@drawable/btn_gest" />
            </LinearLayout>
        </LinearLayout>
    </SlidingDrawer>

</RelativeLayout>