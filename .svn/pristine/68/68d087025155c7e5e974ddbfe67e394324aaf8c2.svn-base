<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:skin="http://schemas.android.com/android/skin"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.htmitech.proxy.activity.DeviceVerifyPasswordActivity">

    <include
        android:id="@+id/system_title"
        layout="@layout/system_title" />

    <com.htmitech.MyView.EmptyLayout
        android:id="@+id/content_root"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F8F8F8">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#F8F8F8"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:paddingTop="20dp">

                <TextView
                    android:id="@+id/tv_device_description"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:lineSpacingExtra="5dp"
                    android:textColor="#666666"
                    android:textSize="13sp" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/ll_add_photo"
                android:layout_width="200dp"
                android:layout_height="200dp"
                android:background="@color/white"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="40dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/iv_add_photo"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_gravity="center_horizontal"
                    android:paddingTop="20dp"
                    android:layout_marginLeft="25dp"
                    android:src="@drawable/img_take_photo"/>

            </LinearLayout>
            <ImageView
                android:id="@+id/iv_photo_detail"
                android:layout_width="300dp"
                android:layout_height="300dp"
                android:src="@drawable/pictures_no"
                android:layout_gravity="center_horizontal"
                android:visibility="gone"
                android:layout_marginTop="40dp"
                >

            </ImageView>


        </LinearLayout>

    </com.htmitech.MyView.EmptyLayout>

</LinearLayout>
