<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    

    <TextView android:id="@+id/keysn_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
		android:layout_alignParentTop="true"
		android:layout_marginLeft="8dp"
		android:layout_marginTop="8dp"
		android:gravity="center"
        android:text="@string/keysn"
	    android:textColor="@android:color/black"
	    android:textSize="18sp"/>
    
    <TextView android:id="@+id/pwd_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/keysn_txt"
        android:layout_alignRight="@id/keysn_txt"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="@string/password"
	    android:textColor="@android:color/black"
	    android:textSize="18sp"/>
    
    <TextView android:id="@+id/url_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/pwd_txt"
        android:layout_alignRight="@id/pwd_txt"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="@string/service_url"
	    android:textColor="@android:color/black"
	    android:textSize="18sp"/>
    
    <EditText 
        android:id="@+id/keysn_edit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/keysn_txt"
        android:layout_alignTop="@id/keysn_txt"
        android:textColor="@android:color/black"
	    android:textSize="18sp"/>
    
    <EditText
        android:id="@+id/pwd_edit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/pwd_txt"
        android:layout_alignTop="@id/pwd_txt"
        android:textColor="@android:color/black"
	    android:textSize="18sp"
	    android:password="true"/>
    
    <EditText
        android:id="@+id/url_edit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/url_txt"
        android:layout_alignTop="@id/url_txt"
        android:textColor="@android:color/black"
	    android:textSize="18sp"/>
</RelativeLayout>
