<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:from="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000931"
    tools:context=".MainActivity">

    <RelativeLayout
        android:id="@+id/ll_delivery_title_theme"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_height"
        android:layout_alignParentTop="true"
        android:background="@color/title_color"
        android:gravity="center_vertical">

        <ImageButton
            android:id="@+id/ib_delivery_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="#00000000"
            android:gravity="center"
            android:src="@drawable/btn_back" />

        <TextView
            android:id="@+id/tv_delivery_title_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:text="发货动态"
            android:textColor="#ffffff"
            android:textSize="@dimen/title_textsize" />

        <TextView
            android:id="@+id/tv_delivery_shaixuan"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="@dimen/title_right_toright"
            android:background="#002B53"
            android:gravity="center"
            android:text="筛选"
            android:textSize="18sp"
            android:visibility="gone" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/ll_delivery_top2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/date_height"
        android:layout_marginTop="@dimen/title_height"
        android:background="#E9F3FF"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">

        <ImageView
            android:id="@+id/iv_delivery_cut"
            android:layout_width="@dimen/datesize"
            android:layout_height="@dimen/datesize"
            android:src="@drawable/date_left" />

        <TextView
            android:id="@+id/tv_delivery_title_data"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="2012年5月"
            android:textColor="@color/black"
            android:textSize="@dimen/date_text_size" />

        <ImageView
            android:id="@+id/iv_delivery_add"
            android:layout_width="@dimen/datesize"
            android:layout_height="@dimen/datesize"
            android:src="@drawable/date_right" />
    </LinearLayout>

    <TextView
        android:id="@+id/tv_delivery_unit_one"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/ll_delivery_top2"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        android:text=""
        android:textColor="#FFF8F8F8"
        android:textSize="16sp" />

    <mobilereport.com.chatkit.myView.FromLayout
        android:id="@+id/fromlayout_delivery_unit_one"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/tv_delivery_unit_one"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="12dp"
        android:orientation="vertical"
        from:from_text_align="center"
        from:from_title="新发折断数量"
        from:from_title_back="#00000000"
        from:from_title_text_align="center"
        from:from_title_text_color="#FFF8F8F8"
        from:from_title_text_size="16sp"
        from:from_watermark_back_color="#C5C1AA"
        from:from_watermark_displayType="repeat"
        from:from_watermark_show="false"
        from:from_watermark_text="">

    </mobilereport.com.chatkit.myView.FromLayout>

    <mobilereport.com.chatkit.myView.FromLayout
        android:id="@+id/fromlayout_delivery_unit_twofix"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/tv_delivery_unit_one"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="12dp"
        android:orientation="vertical"
        from:from_text_align="center"
        from:from_title="新发折断数量"
        from:from_title_back="#00000000"
        from:from_title_text_align="center"
        from:from_title_text_color="#FFF8F8F8"
        from:from_title_text_size="16sp"
        from:from_watermark_back_color="#C5C1AA"
        from:from_watermark_displayType="repeat"
        from:from_watermark_show="false"
        from:from_watermark_text="">

    </mobilereport.com.chatkit.myView.FromLayout>

    <mobilereport.com.chatkit.myView.FromLayout
        android:id="@+id/fromlayout_delivery_unit_twonofixorthreefix"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/tv_delivery_unit_one"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="12dp"
        android:orientation="vertical"
        from:from_text_align="center"
        from:from_title="新发折断数量"
        from:from_title_back="#00000000"
        from:from_title_text_align="center"
        from:from_title_text_color="#FFF8F8F8"
        from:from_title_text_size="16sp"
        from:from_watermark_back_color="#C5C1AA"
        from:from_watermark_displayType="repeat"
        from:from_watermark_show="false"
        from:from_watermark_text="">

    </mobilereport.com.chatkit.myView.FromLayout>

    <mobilereport.com.chatkit.myView.FromLayout
        android:id="@+id/formlayout_twonofixorthreenofixfourfix"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/tv_delivery_unit_one"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="12dp"
        android:orientation="vertical"
        from:from_text_align="center"
        from:from_title="新发折断数量"
        from:from_title_back="#00000000"
        from:from_title_text_align="center"
        from:from_title_text_color="#FFF8F8F8"
        from:from_title_text_size="16sp"
        from:from_watermark_back_color="#C5C1AA"
        from:from_watermark_displayType="repeat"
        from:from_watermark_show="false"
        from:from_watermark_text="">

    </mobilereport.com.chatkit.myView.FromLayout>


    <mobilereport.com.chatkit.myView.FromLayout
        android:id="@+id/fromlayout_delivery_unit_generalfix"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/tv_delivery_unit_one"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="12dp"
        android:orientation="vertical"
        from:from_text_align="center"
        from:from_title="新发折断数量"
        from:from_title_back="#00000000"
        from:from_title_text_align="center"
        from:from_title_text_color="#FFF8F8F8"
        from:from_title_text_size="16sp"
        from:from_watermark_back_color="#C5C1AA"
        from:from_watermark_displayType="repeat"
        from:from_watermark_show="false"
        from:from_watermark_text="">

    </mobilereport.com.chatkit.myView.FromLayout>

</RelativeLayout>