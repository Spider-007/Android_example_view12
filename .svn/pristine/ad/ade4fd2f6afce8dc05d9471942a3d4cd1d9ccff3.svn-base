<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:descendantFocusability="blocksDescendants"
    android:orientation="horizontal" >

    <TextView
        android:layout_width="@dimen/tableline"
        android:layout_height="@dimen/tableheight"
        android:background="@color/colortable"
        android:gravity="center" />

    <TextView
        android:id="@+id/tv_complete_xianming"
        android:layout_width="@dimen/chejiantablewidth"
        android:layout_height="@dimen/tableheight"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="线名"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/white"
        android:textSize="@dimen/chejiantextsize" />

    <LinearLayout
        android:id="@+id/zt_layout_sum_item"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/tv_complete_xianming"
        android:orientation="vertical" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/tableline"
        android:layout_alignBottom="@id/zt_layout_sum_item"
        android:layout_alignRight="@id/zt_layout_sum_item"
        android:layout_gravity="center"
        android:background="@color/colortable"
        android:gravity="center" />

</RelativeLayout>