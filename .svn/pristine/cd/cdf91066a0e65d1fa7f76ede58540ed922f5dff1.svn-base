package com.htmitech.ztcustom.zt.domain;
/**
 * IB06_GetStatParam-获得统计参数
 * 设备伤损情况汇总功能，默认月份，超过财务日就应该自动显示下个月！
 * @author htrf-pc
 *
 */
public class GetStatParam {
	public boolean success;
	public String msg;
	public String stat_day;
	public boolean isSuccess() {
		return success;
	}
	public void setSuccess(boolean success) {
		this.success = success;
	}
	public String getMsg() {
		return msg;
	}
	public void setMsg(String msg) {
		this.msg = msg;
	}
	public String getStat_day() {
		return stat_day;
	}
	public void setStat_day(String stat_day) {
		this.stat_day = stat_day;
	}
	
	
}
