public class CompleteSumCardContant {
	private String orgname;
	private double ggts_sum;
	private double dcts_sum;
	private double hfts_sum;

	public String getOrgname() {
		return orgname;
	}

	public void setOrgname(String orgname) {
		this.orgname = orgname;
	}

	public double getGgts_sum() {
		return ggts_sum;
	}

	public void setGgts_sum(double ggts_sum) {
		this.ggts_sum = ggts_sum;
	}

	public double getDcts_sum() {
		return dcts_sum;
	}

	public void setDcts_sum(double dcts_sum) {
		this.dcts_sum = dcts_sum;
	}

	public double getHfts_sum() {
		return hfts_sum;
	}

	public void setHfts_sum(double hfts_sum) {
		this.hfts_sum = hfts_sum;
	}

}
