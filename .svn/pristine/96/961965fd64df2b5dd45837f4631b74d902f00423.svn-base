<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/allbackground"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/rl_quality_objection_deal_ove_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp">

        <CheckBox
            android:id="@+id/cb_quality_objection_deal_over"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:button="@drawable/quality_objection_deal_over_checkbox_seletor"
            android:clickable="false"
            android:enabled="false"
            android:focusable="false" />


        <ImageView
            android:id="@+id/iv_quality_objection_deal_over_pic"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:layout_toRightOf="@id/cb_quality_objection_deal_over"
            android:src="@drawable/img_zlyy_list_thumbnail" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/iv_quality_objection_deal_over_pic"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/iv_quality_objection_deal_over_pic">

            <TextView
                android:id="@+id/tv_quality_objection_deal_over_code"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="50dp"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="2018-001"
                android:textColor="#F8F8F8"
                android:textSize="14sp" />


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/iv_quality_objection_deal_over_pic">

            <TextView
                android:id="@+id/tv_quality_objection_deal_over_title_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="50dp"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="中国铁路哈尔滨局集团有限公司"
                android:textColor="#F8F8F8"
                android:textSize="14sp" />


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/iv_quality_objection_deal_over_pic"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/iv_quality_objection_deal_over_pic">

            <TextView
                android:id="@+id/tv_quality_objection_deal_over_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="50dp"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="60N 焊轨 鞍钢asdfasdf"
                android:textColor="#D5D5D5"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tv_quality_objection_deal_over_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:gravity="center"
                android:text="2018年13月13日"
                android:textColor="#9C9C9C"
                android:textSize="12sp" />

        </RelativeLayout>
    </RelativeLayout>

</LinearLayout>