<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <com.htmitech.MyView.EmptyLayout
        android:id="@+id/layout_search_no"
        android:layout_width="match_parent"
        android:background="#fff"
        android:layout_height="match_parent">

        <com.htmitech.emportal.ui.detail.ObservableScrollView
            android:id="@+id/sc_freement"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <LinearLayout
                android:id="@+id/ll_parent"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_source_content"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dp"
                    android:lineSpacingMultiplier="1.5"
                    android:text=""
                    android:textColor="@color/color_55555555"
                    android:textSize="14sp"
                    android:layout_marginLeft="20dp"
                    android:visibility="gone"
                    android:layout_marginRight="20dp"/>
            </LinearLayout>


        </com.htmitech.emportal.ui.detail.ObservableScrollView>
    </com.htmitech.MyView.EmptyLayout>



</RelativeLayout>