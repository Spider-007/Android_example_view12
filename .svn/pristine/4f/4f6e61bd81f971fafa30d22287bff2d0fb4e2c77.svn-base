<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:skin="http://schemas.android.com/android/skin"
    android:layout_width="fill_parent"
    android:id="@+id/parentLayout"
    android:layout_height="fill_parent"
    android:background="@color/bg_grey_dark">

    <RelativeLayout
        android:id="@+id/rl_strues"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_bar_height"
        skin:enable="true"
        android:background="@color/ht_hred_title">


        <ImageView
            android:id="@+id/iv_back"
            style="@style/title_button"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            skin:enable="true"
            android:scaleType="center"
            android:background="@drawable/mx_btn_back" />
        <ImageView
            android:id="@+id/iv_back_home"
            style="@style/title_button"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            skin:enable="true"
            android:scaleType="center"
            android:visibility="gone"
            android:background="@drawable/mx_btn_community_list_phone" />

        <TextView
            android:id="@+id/tv_names"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/add_application_center_title"
            android:textColor="@color/ht_title_text_color"
            skin:enable="true"
            android:textSize="18dp" />
        <ProgressBar
            android:id="@+id/progress_"
            style="?android:attr/progressBarStyleSmall"
            android:layout_width="15dip"
            android:layout_height="15dip"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@id/tv_names"
            android:visibility="gone"
            android:indeterminateDrawable="@anim/imofan_round_progress_bar_1" />
        <ImageView
            android:id="@+id/iv_menu"
            style="@style/title_button"
            android:layout_alignParentRight="true"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:scaleType="center"
            skin:enable="true"
            android:background="@drawable/mx_btn_add" />

        <TextView
            android:id="@+id/complete"
            style="@style/title_button"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:text="完成"
            android:textColor="@color/white"
            android:visibility="gone" />
    </RelativeLayout>

    <com.htmitech.app.widget.UserMessageScrollView
        android:id="@+id/scroll_view"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:overScrollMode="never"
        android:focusable="false"
        android:fadingEdge="none"
        android:scrollbars="none"
        android:layout_below="@+id/rl_strues">

        <RelativeLayout
            android:id="@+id/ll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:focusable="true"
            android:focusableInTouchMode="true"
            >

            <com.htmitech.emportal.ui.applicationcenter.DragGrid
                android:id="@+id/gradview"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/bg_grey_dark"
                android:gravity="center"
                android:clickable="true"
                android:longClickable="true"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:listSelector="@android:color/transparent"
                android:numColumns="3"
                android:padding="1dp"
                android:scrollbars="vertical"
                android:stretchMode="columnWidth">
                <!--android:layoutAnimation="@anim/ht_gridview_anim"-->
            </com.htmitech.emportal.ui.applicationcenter.DragGrid>

            <com.htmitech.MyView.ApplicationCenterListChildLayout
                android:id="@+id/ll_app_center_list_child"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:focusable="false"
                android:clickable="false"
                android:orientation="vertical" />


        </RelativeLayout>
    </com.htmitech.app.widget.UserMessageScrollView>
    <LinearLayout
        android:id="@+id/ll_child_top"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/app_center_list_child_back"
        android:gravity="center_vertical"
        android:visibility="gone"
        android:layout_below="@+id/rl_strues"
        android:orientation="horizontal">

        <TextView
            android:layout_width="4dp"
            android:layout_height="20dp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="15dp"
            android:background="#F9C80E" />

        <TextView
            android:id="@+id/tv_app_center_classify_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="办公应用"
            android:textColor="@color/color_55555555"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/iv_app_center_classify_jt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleType="fitCenter"
            android:src="@drawable/btn_angle_up_circle" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/layout_search_no"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="160dp"
        android:layout_gravity="center"
        android:visibility="gone"
        android:orientation="vertical">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:src="@drawable/img_no_application"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:textColor="@color/black"
            android:layout_gravity="center"
            android:textSize="15sp"
            android:text="@string/seratch_nots1"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:textColor="#909090"
            android:layout_gravity="center"
            android:textSize="13sp"
            android:text="@string/seratch_nots2"/>
    </LinearLayout>
</RelativeLayout>