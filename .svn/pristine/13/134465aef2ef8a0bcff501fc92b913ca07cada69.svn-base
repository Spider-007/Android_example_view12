<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center_vertical"
    android:background="@drawable/corners_bg_white_press_stroke"
    android:gravity="center_vertical"
    android:minHeight="50dp"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/form_fielditem_text"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_margin="2dp"
        android:textIsSelectable="true"
        android:gravity="center_vertical"
        android:textColor="@color/color_55555555"
        android:textSize="17sp"></TextView>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_weight="2.1"
        android:id="@+id/ll_edit_fielditem"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/form_fielditem_beforetext"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_margin="2dp"
            android:gravity="center_vertical"
            android:textIsSelectable="true"
            android:text=""
            android:textColor="@color/color_55555555"
            android:textSize="17sp"></TextView>

        <FrameLayout
            android:id="@+id/fl_fielditem_editvalue"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_margin="2dp"
            android:visibility="gone"
            android:layout_weight="1">
            <com.htmitech.edittext.SuperEditText
                android:id="@+id/form_fielditem_editvalue"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center_vertical"
                android:background="@drawable/edittext_select_back"
                android:textColor="@color/color_55555555"
                android:textSize="19sp"
                android:visibility="gone"></com.htmitech.edittext.SuperEditText>
            <View
                android:id="@+id/view_edit_fielditem"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@android:color/transparent"
                android:visibility="gone"/>

        </FrameLayout>



        <com.htmitech.edittext.SuperEditText
            android:id="@+id/form_fielditem_supperedit"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:layout_weight="1"
            android:textColor="@color/color_55555555"
            android:textSize="19sp"
            android:visibility="gone"
            android:background="@drawable/edittext_select_back"
            >

        </com.htmitech.edittext.SuperEditText>

        <com.htmitech.edittext.SuperEditText
            android:id="@+id/form_fielditem_mle"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:layout_weight="1"
            android:background="@drawable/edittext_select_back"
            android:textColor="@color/color_55555555"
            android:textSize="19sp"
            android:visibility="gone">

        </com.htmitech.edittext.SuperEditText>

        <com.htmitech.edittext.SuperEditText
            android:id="@+id/form_fielditem_met"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:layout_weight="1"
            android:background="@drawable/edittext_select_back"
            android:textColor="@color/color_55555555"
            android:textSize="19sp"
            android:visibility="gone">

        </com.htmitech.edittext.SuperEditText>

        <com.htmitech.edittext.SuperEditText
            android:id="@+id/form_fielditem_net"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:layout_weight="1"
            android:background="@drawable/edittext_select_back"
            android:textColor="@color/color_55555555"
            android:textSize="19sp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/form_fielditem_endtext"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_margin="2dp"
            android:gravity="center_vertical"
            android:textIsSelectable="true"
            android:text=""
            android:textColor="@color/color_55555555"
            android:textSize="17sp"
            android:visibility="invisible"></TextView>

        <ImageView
            android:id="@+id/form_fielditem_editimage"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginRight="5dp"
            android:layout_gravity="center_vertical"
            android:paddingTop="5dp"
            android:src="@drawable/ic_editflag"
            android:visibility="gone" />
    </LinearLayout>



</LinearLayout>