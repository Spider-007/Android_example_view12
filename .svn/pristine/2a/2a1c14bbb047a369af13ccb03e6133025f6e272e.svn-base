<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_ffffffff"
    android:orientation="vertical"
    android:paddingBottom="10dp"
    android:paddingLeft="5dp"
    android:paddingRight="5dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/iv_faqi_type"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_gravity="center_vertical"
            android:src="@drawable/icon_email" />

        <TextView
            android:id="@+id/tv_faqi_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="7dp"
            android:maxLines="2"
            android:singleLine="false"
            android:text="h关于h关于h关于h关于h"
            android:textColor="@color/color_ff333333"
            android:textSize="@dimen/fontsize_heading15" />
    </LinearLayout>


    <LinearLayout
        android:id="@+id/linearlayout_faqi_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="6dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">


        <LinearLayout
            android:id="@+id/linearlayout_faqi_2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="7dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linearlayout_faqi_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_faqi_committer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="提交人:"
                    android:textColor="@color/color_ff666666"
                    android:textSize="@dimen/fontsize_heading12" />

                <TextView
                    android:id="@+id/tv_faqi_sendfrom"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:textColor="@color/color_ff666666"

                    android:textSize="@dimen/fontsize_heading12" />

                <TextView
                    android:id="@+id/tv_faqi_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:maxLength="12"
                    android:text="111ddddd1"
                    android:textColor="@color/color_ff666666"
                    android:textSize="@dimen/fontsize_heading12" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearlayout_faqi_4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textColor="@color/color_ff666666"
                    android:textSize="@dimen/fontsize_heading12" />

                <TextView
                    android:id="@+id/tv_faqi_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="shijian"
                    android:textColor="@color/color_ff666666"
                    android:textSize="@dimen/fontsize_heading12" />

                <TextView
                    android:id="@+id/tv_faqi_doc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="查看正文 >"
                    android:textColor="@color/color_ff666666"
                    android:textSize="@dimen/fontsize_heading12" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>


</LinearLayout>






