<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/allbackground"
    tools:context="com.htmitech.ztcustom.zt.fragment.qualityobjectionfragment.QualityObjectionFGSSLFragment">

    <com.htmitech.commonx.pulltorefresh.library.PullToRefreshListView
        android:id="@+id/lv_quality_objection_FGSSL"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#000931"
        android:layout_marginTop="5dp"
        android:cacheColorHint="#00000000"
        android:divider="#00000000"
        android:dividerHeight="0dp"
        android:fadingEdge="none"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="center"
        android:listSelector="#00000000"
        android:scrollbars="none"
        android:textColor="#fff">

    </com.htmitech.commonx.pulltorefresh.library.PullToRefreshListView>

</FrameLayout>
