<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.archivermodule">
    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="15"
        tools:overrideLibrary="htmitech.com.componentlibrary"/>
    <uses-feature
        android:name="android.hardware.touchscreen"
        android:required="false" />
    <application>
        <activity android:name="com.example.archivermodule.ArchiverModuleActivity">
        </activity>
        <activity
            android:name="com.example.archivermodule.BookShower"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:launchMode="singleTop"
            android:process="com.kinggrid.PDFShow"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan|stateHidden"></activity>
    </application>

</manifest>