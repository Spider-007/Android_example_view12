<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/allbackground">


    <RelativeLayout
        android:id="@+id/ll_quality_book_title_theme"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_height"
        android:layout_alignParentTop="true"
        android:background="@color/title_color"
        android:gravity="center_vertical">

        <ImageButton
            android:id="@+id/ib_quality_book_back"
            android:clickable="true"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="#00000000"
            android:gravity="center"
            android:src="@drawable/btn_back" />

        <TextView
            android:id="@+id/tv_quality_book_title_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="质保书"
            android:textColor="#ffffff"
            android:textSize="@dimen/title_textsize" />

        <Button
            android:id="@+id/bt_quality_book_right_top"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_marginRight="8dp"
            android:background="@color/transparent"
            android:text="提报"
            android:textColor="#F8F8F8"
            android:textSize="@dimen/quality_objection_top_textsize"
            android:visibility="gone" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/ll_qualitybook_searchnews"
        android:layout_below="@+id/ll_quality_book_title_theme"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#F0EFF7"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="搜索信息"
            android:textSize="12sp"
            android:textColor="#333333"
            android:layout_marginTop="4dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="5dp"/>
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/rl_qualitybook1"
        android:layout_below="@+id/ll_qualitybook_searchnews"
        android:layout_width="match_parent"
        android:layout_height="46dp">
        <ImageView
            android:id="@+id/img_bulide"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_jiansdw"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp" />
        <TextView
            android:id="@+id/tv_builde"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="建设单位"
            android:layout_toRightOf="@+id/img_bulide"
            android:textSize="14sp"
            android:textColor="#F8F8F8"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"/>
        <ImageView
            android:id="@+id/img_next1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_clzz_next"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="12dp"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_qualitybook1_1"
        android:layout_below="@+id/rl_qualitybook1"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:layout_marginLeft="40dp"
        android:visibility="gone">
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#3E5CE0"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="75dp"
            android:layout_centerVertical="true"
            android:background="@drawable/qualitybook_searched_news">
            <TextView
                android:id="@+id/tv_builder"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textSize="11sp"
                android:textColor="#000000"
                android:layout_toLeftOf="@+id/img_clean1"
                android:layout_marginLeft="13dp"/>
            <ImageView
                android:id="@+id/img_clean1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_clean"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="13dp"/>
        </RelativeLayout>
    </RelativeLayout>

    <View
        android:id="@+id/v1"
        android:layout_below="@+id/rl_qualitybook1_1"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#3E5CE0"/>

    <RelativeLayout
        android:id="@+id/rl_qualitybook2"
        android:layout_below="@+id/v1"
        android:layout_width="match_parent"
        android:layout_height="46dp">
        <ImageView
            android:id="@+id/img_shouhuo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_shouhdw"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp" />
        <TextView
            android:id="@+id/tv_shouhuo"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="收货单位"
            android:layout_toRightOf="@+id/img_shouhuo"
            android:textSize="14sp"
            android:textColor="#F8F8F8"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"/>
        <ImageView
            android:id="@+id/img_next2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_clzz_next"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="12dp"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_qualitybook2_2"
        android:layout_below="@+id/rl_qualitybook2"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:layout_marginLeft="40dp"
        android:orientation="vertical"
        android:visibility="gone">
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#3E5CE0"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="75dp"
            android:layout_centerVertical="true"
            android:background="@drawable/qualitybook_searched_news">
            <TextView
                android:id="@+id/tv_receivegoods"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/img_clean2"
                android:textSize="11sp"
                android:textColor="#000000"
                android:layout_marginLeft="13dp"/>
            <ImageView
                android:id="@+id/img_clean2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_clean"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="13dp"/>
        </RelativeLayout>
    </RelativeLayout>

    <View
        android:id="@+id/v2"
        android:layout_below="@+id/rl_qualitybook2_2"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#3E5CE0"/>

    <RelativeLayout
        android:id="@+id/rl_qualitybook3"
        android:layout_below="@+id/v2"
        android:layout_width="match_parent"
        android:layout_height="46dp">
        <ImageView
            android:id="@+id/img_station"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_daozhan"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp" />
        <TextView
            android:id="@+id/tv_station"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="到站"
            android:layout_toRightOf="@+id/img_station"
            android:textSize="14sp"
            android:textColor="#F8F8F8"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"/>
        <ImageView
            android:id="@+id/img_next3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_clzz_next"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="12dp"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_qualitybook3_3"
        android:layout_below="@+id/rl_qualitybook3"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:layout_marginLeft="40dp"
        android:orientation="vertical"
        android:visibility="gone">
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#3E5CE0"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="75dp"
            android:layout_centerVertical="true"
            android:background="@drawable/qualitybook_searched_news">
            <TextView
                android:id="@+id/tv_stations"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/img_clean3"
                android:textSize="11sp"
                android:textColor="#000000"
                android:layout_marginLeft="13dp"/>
            <ImageView
                android:id="@+id/img_clean3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/icon_clean"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="13dp"/>
        </RelativeLayout>
    </RelativeLayout>

    <View
        android:id="@+id/v3"
        android:layout_below="@+id/rl_qualitybook3_3"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#3E5CE0"/>

    <RelativeLayout
        android:id="@+id/rl_qualitybook4"
        android:layout_below="@+id/v3"
        android:layout_width="match_parent"
        android:layout_height="46dp">
        <ImageView
            android:id="@+id/img_qualitybooknum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_zhibsbh"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp" />
        <TextView
            android:id="@+id/tv_qualitybooknum"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="质保书编号"
            android:layout_toRightOf="@+id/img_qualitybooknum"
            android:textSize="14sp"
            android:textColor="#F8F8F8"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_qualitybook4_4"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:orientation="vertical"
        android:visibility="gone"
        android:layout_below="@+id/rl_qualitybook4"
        android:layout_marginLeft="40dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#3E5CE0"/>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="75dp"
            android:background="@drawable/qualitybook_numsearch_shape">
            <EditText
                android:id="@+id/et_search_bianhao"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="请您在此输入"
                android:layout_centerVertical="true"
                android:background="@null"
                android:textSize="11sp"
                android:singleLine="true"
                android:imeOptions="actionNext"
                android:inputType="number"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:textColorHint="#FFFFFF"
                android:textColor="#FFFFFF"
                android:layout_marginLeft="11dp"/>
        </RelativeLayout>
    </RelativeLayout>

    <View
        android:id="@+id/v4"
        android:layout_below="@+id/rl_qualitybook4_4"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#3E5CE0"/>

    <RelativeLayout
        android:id="@+id/rl_qualitybook5"
        android:layout_below="@+id/v4"
        android:layout_width="match_parent"
        android:layout_height="46dp">
        <ImageView
            android:id="@+id/img_carnum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_chehao"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp" />
        <TextView
            android:id="@+id/tv_carnum"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="车号"
            android:layout_toRightOf="@+id/img_carnum"
            android:textSize="14sp"
            android:textColor="#F8F8F8"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_qualitybook5_5"
        android:layout_below="@+id/rl_qualitybook5"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:layout_marginLeft="40dp"
        android:orientation="vertical"
        android:visibility="gone">
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#3E5CE0"/>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="75dp"
            android:background="@drawable/qualitybook_numsearch_shape">
            <EditText
                android:id="@+id/et_search_carnum"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="请您在此输入"
                android:layout_centerVertical="true"
                android:background="@null"
                android:textSize="11sp"
                android:singleLine="true"
                android:imeOptions="actionSearch"
                android:inputType="number"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:textColor="#FFFFFF"
                android:textColorHint="#FFFFFF"
                android:layout_marginLeft="11dp"/>
        </RelativeLayout>

    </RelativeLayout>

    <View
        android:id="@+id/v5"
        android:layout_below="@+id/rl_qualitybook5_5"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#3E5CE0"/>


    <Button
        android:id="@+id/btn_search"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:text="搜 索"
        android:paddingTop="14dp"
        android:paddingBottom="15dp"
        android:background="#022453"
        android:textSize="18sp"
        android:textColor="#FFFFFF"/>



</RelativeLayout>