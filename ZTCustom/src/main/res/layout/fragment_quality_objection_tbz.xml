<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/allbackground"
    tools:context="com.htmitech.ztcustom.zt.fragment.qualityobjectionfragment.QualityObjectionTBZSFragment">

    <com.htmitech.commonx.pulltorefresh.library.PullToRefreshListView
        android:id="@+id/lv_quality_objection_TBZS"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="5dp"
        android:background="#000931"
        android:cacheColorHint="#00000000"
        android:divider="#00000000"
        android:dividerHeight="0dp"
        android:fadingEdge="none"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="center"
        android:listSelector="#00000000"
        android:scrollbars="none"
        android:textColor="#fff">

    </com.htmitech.commonx.pulltorefresh.library.PullToRefreshListView>

    <TextView
        android:id="@+id/tv_quality_objection_TBZS_no_value"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:text="暂无数据"
        android:textColor="@color/white"
        android:textSize="@dimen/date_text_size" />

</FrameLayout>
