<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/context_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">


     <!-- title -->

    <include
        android:id="@+id/system_title_pie"
        layout="@layout/system_title" />
    <!-- timebar -->


    
    
    <LinearLayout
        android:id="@+id/layout_pie_timebar"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="#E9F3FF"
        android:orientation="horizontal" >

        <ImageButton
            android:id="@+id/before_button_pie"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/btn_prev_pre"
            android:visibility="gone" />

        <TextView
            android:id="@+id/supply_time_textview_pie"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="60"
            android:gravity="center"
            android:textColor="@color/black"
            android:textSize="19sp" 
            android:text="2015å¹´"/>

        <ImageButton
            android:id="@+id/behind_button_pie"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/btn_next_pre"
            android:visibility="gone" />
    </LinearLayout>
    
     <TextView
            android:id="@+id/supply_time_textview_charttitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|left"
            android:paddingLeft="10dp"
            android:background="@color/drawer_back"
            android:textColor="@color/white"
            android:textSize="18sp" 
            android:text=""/>

    <LinearLayout
        android:id="@+id/main_content_pie"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/tab_menu_pie"
        android:background="@color/drawer_back"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/relative_layout_pie"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/transparent" >

            <LinearLayout
                android:id="@+id/frame_chart_table"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="vertical" />


        </RelativeLayout>
    </LinearLayout>

</LinearLayout>